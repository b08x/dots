  "ollamaCodeFixer.contextLength": 8192,
  "ollamaCodeFixer.maxTokens": 4096,
  "ollamaCodeFixer.temperature": 0.3,
  "diffEditor.codeLens": true,
  "sublayer-blueprints.host": "http://ninjabot:81",
