{
  "ollamaCodeFixer.ollamaApiUrl": "http://gir:11434",
  "github.copilot.chat.byok.ollamaEndpoint": "http://gir:11434",
  "ollama.apiHost": "http://gir:11434",
  "ollama.defaultModel": "qwen2.5-coder:7b",
  "ollamaCodeFixer.preferredModels": [
    "codellama:7b",
    "codellama:13b",
    "codegemma:7b",
    "llama2:7b",
    "mistral:7b",
    "qwen2.5-coder:7b",
    "gemma3:4b"
  ],
  "ollama-commit.apiUrl": "http://gir:11434/api/generate",
