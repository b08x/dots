  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": [
        "--login"
      ]
    },
    "fish": {
      "path": "fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "redhat.telemetry.enabled": false,
  "codium.editorButton": false,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "pgsql.serverGroups": [
    {
      "name": "Servers",
      "id": "3F755F4A-0B35-41E3-9EB6-1AC5C74C7718",
      "isDefault": true
    }
  ],
  "ollamaCodeFixer.autoStartOllama": false,
